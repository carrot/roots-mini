{
  "name": "roots-mini",
  "description": "a little roots-y experiment",
  "version": "0.0.0",
  "author": "Jeff Escalante",
  "bugs": {
    "url": "https://github.com/carrot/roots-mini/issues"
  },
  "dependencies": {
    "argparse": "^1.0.6",
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "css-loader": "^0.23.1",
    "glob": "^6.0.1",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "joi": "^7.1.0",
    "micromatch": "^2.3.7",
    "postcss": "^5.0.12",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.0",
    "precss": "^1.3.0",
    "require-from-string": "^1.1.0",
    "webpack": "^1.12.10",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-eslint": "^4.1.6",
    "husky": "^0.10.2",
    "nyc": "^5.0.1",
    "rimraf": "^2.4.5",
    "snazzy": "^2.0.1",
    "standard": "^6.0.4"
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "homepage": "https://github.com/carrot/roots-mini",
  "keywords": [
    "roots",
    "static",
    "webpack"
  ],
  "license": "MIT",
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/carrot/roots-mini.git"
  },
  "scripts": {
    "coverage": "nyc --reporter=html ava && open coverage/index.html",
    "lint": "standard --verbose | snazzy",
    "precommit": "npm run lint -s",
    "pretest": "npm run lint -s",
    "test": "ava --require babel-core/register --verbose --serial",
    "build": "mv lib src && babel src --out-dir lib",
    "unbuild": "rm -rf lib && mv src lib"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
